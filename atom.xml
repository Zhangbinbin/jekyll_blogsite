<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Bin Zhang</title>
 <link href="http://binzhang.tk/atom.xml" rel="self"/>
 <link href="http://binzhang.tk/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://binzhang.tk/</id>
 <author>
     <name>Bin Zhang</name>
   <email>yugong0391@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://binzhang.tk{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://binzhang.tk{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
